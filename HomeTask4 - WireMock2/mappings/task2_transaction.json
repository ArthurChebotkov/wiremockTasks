{
    "request": {
        "urlPathPattern": "/transaction/",
    "queryParameters": {
            "price": {
                "matches": "[0-9]*"
            },
			"date": {
				"matches": "([0-9]{3}[1-9]|[0-9]{2}[1-9][0-9]{1}|[0-9]{1}[1-9][0-9]{2}|[1-9][0-9]{3})-(0[1-9]|[1][0-2]|)-(0[1-9]|[12][0-9]|3[01])"
			}
        },
        "method": "GET"
    },
    "response": {
        "status": 200,
		"jsonBody": "Everything was just fine!"
    }
}
